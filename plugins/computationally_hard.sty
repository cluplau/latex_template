% Complexity classes
\newcommand{\NP}{{\mathcal{NP}}}
\newcommand{\PP}{{\mathcal{PP}}}
\newcommand{\RP}{{\mathcal{RP}}}
\newcommand{\BPP}{{\mathcal{BPP}}}
\newcommand{\ZPP}{{\mathcal{ZPP}}}
\newcommand{\Po}{{\mathcal{P}}}

% Types of numbers
\newcommand{\NN}{{\mathbb N}}
\newcommand{\RR}{{\mathbb R}}
\newcommand{\EE}{{\mathbb E}}
\newcommand{\QQ}{{\mathbb Q}}
\newcommand{\ZZ}{{\mathbb Z}}
\newcommand{\CC}{{\mathbb C}}

\newcommand{\vect}[1]{{\bm{#1}}}
\newcommand{\pname}[1]{{\ifmmode \textsc{#1}\else\normalfont\scshape #1\fi}}
\newcommand{\npcn}{$\NP$-completeness\xspace}
\newcommand{\npc}{$\NP$-complete\xspace}
\newcommand{\polr}{\le_p}
\newcommand{\len}[1]{\norm{#1}}
\newcommand{\size}[1]{\Norm{#1}}

% Helpers
\newcommand{\norm}[1]{\left\vert#1\right\vert}
\newcommand{\Norm}[1]{\left\Vert#1\right\Vert}
\newcommand{\ms}[1]{\texttt{#1}}
\newcommand{\command}[2]{\ms{\textbackslash #1} & #2 \\ \hline}


